{
  "name": "csscountrycodes",
  "version": "2.0.0",
  "description": "a dropdown css for countries",
  "main": "index.js",
  "repository": "https://github.com/Arnoud-B/csscountrycodes.git",
  "author": "https://github.com/Arnoud-B/csscountrycodes/graphs/contributors",
  "license": "MIT",
  "contributors": [
    "Arnoud-B",
    "Jay DevOps LLC"
  ],
  "scripts": {
    "css": "npm-run-all --parallel css-lint* css-compile* --sequential css-prefix* css-minify*",
    "css-main": "npm-run-all --parallel css-lint css-compile --sequential css-prefix css-minify",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/flags.scss dist/flags.css",
    "css-compile-docs": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/flags.org.scss dist/flags.org.css",
    "css-lint": "stylelint --syntax scss \"scss/**/*.scss\"",
    "css-lint-vars": "node build/lint-vars.js scss/ ",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/flags.css\" \"!dist/flags.min.css\"",
    "css-prefix-docs": "postcss --config build/postcss.config.js --replace \"dist/flags.org.min.css\" \"dist/flags.org.css\"",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/flags.min.css dist/flags.css",
    "css-minify-docs": "cleancss --level 1 --output dist/flags.org.min.css dist/flags.org.css"
  },
  "style": "dist/flags.css",
  "sass": "scss/flags.scss",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist/",
    "js/{src,dist}/",
    "scss/"
  ],
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "devDependencies": {
    "clean-css-cli": "^4.1.10",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^5.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.3.0"
  }
}
